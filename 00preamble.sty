%Figure Placement - \fig{filepath}{0>linewidth<1}{caption}{type:label}
\newcommand{\fig}[4]
{
    \begin{figure}[H] %places image HERE
    \centering
    \includegraphics[width=#2\linewidth]{images/#1}
    \caption{#3}
    \label{#4}
    \end{figure}
}

\newcommand{\tablefig}[4]
{
    \begin{table}[H]
    \centering
    % \begin{figure} %places image HERE
    \includegraphics[width=#2\linewidth]{images/#1}
    % \end{figure}
    \caption{#3}
    \label{#4}
    \end{table}
}


